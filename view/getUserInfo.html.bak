		//设置js方法调用的父对象为mam

		function onDeviceReady(){
			
						
		}
			var mam = {navigator:navigator};
			//前往页面
			function toPage(target)
		 	{			
				var id = target.id;
				alert("id= "+id);
				if(id=="getUserInfo")
				{
					mam.navigator.loadconfig.getUserInfo(getUserInfoCallback);				
				}else if(id == "loginSession")
				{					
					mam.navigator.loadconfig.loginsession('1', getInfoCallback);
				}
		  	}
			function getUserInfoCallback(obj)
			{							
				var nameCode=obj.nameCode;
				var areaId = obj.areaId;
				var userId=obj.userId;		
				var name=obj.name;
				var baseOrgId=obj.baseOrgId;
				var baseOrgName=obj.baseOrgName;

				alert("统一框架(ISC)登录名: "+nameCode +
				" 区域编号: "+areaId+
				" 用户编号: "+userId+
				" 登录用户姓名: "+name+
				" 用户所属基准组织结构: "+baseOrgId+
				" 用户所属组织机构名称: "+baseOrgName)
				console.log("getUserInfo插件调用成功:" + JSON.stringify(obj) );
			}
			function getInfoCallback(obj)
			{
				console.log("loginSession返回值："+ JSON.stringify(obj));
				alert("loginSession返回值："+ JSON.stringify(obj));
			}

			function successCallback(v)
			{
					var json = JSON.stringify(v);
					alert("getJson插件调用成功: "+json);
			}
		
		</script>
